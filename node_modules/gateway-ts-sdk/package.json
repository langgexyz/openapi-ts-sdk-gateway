{
  "name": "gateway-ts-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for Stream Gateway WebSocket server",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "browser": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "node esbuild.config.js && tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "clean": "rm -rf dist/ *.tgz",
    "rebuild": "npm run clean && npm run build",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "check": "npm run typecheck",
    "examples:node": "node examples/node.cjs",
    "examples:browser": "echo '\n🌐 浏览器示例:\n   📊 Ping + 订阅 + 发布测试: http://localhost:8080/examples/browser.html\n   🔗 代理测试: http://localhost:8080/examples/proxy.html\n' && npx --yes http-server . -p 8080 -c-1",
    "examples:hook-proxy": "node examples/hook-proxy.cjs",
    "test": "node scripts/test.cjs",
    "pack": "npm run check && npm run build && npm pack && mv *.tgz packages/",
    "login": "node scripts/login.cjs",
    "release": "npm run check && npm run build && npm pack && mv *.tgz packages/ && echo '✅ Package created successfully!'"
  },
  "keywords": [
    "websocket",
    "stream",
    "gateway",
    "typescript",
    "sdk",
    "realtime",
    "channel",
    "messaging",
    "browser",
    "nodejs",
    "universal",
    "isomorphic"
  ],
  "author": "Stream Gateway Team",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "esbuild": "^0.25.9",
    "eslint": "^8.50.0",
    "npm-cli-login": "^1.0.0",
    "ts-concurrency": "github:xpwu/ts-concurrency",
    "ts-json": "github:xpwu/ts-json#semver:^0.2.x",
    "ts-streamclient": "github:xpwu/ts-streamclient#semver:^2.0.0",
    "ts-xutils": "github:xpwu/ts-xutils",
    "tslib": "^2.8.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/gateway-go-server.git",
    "directory": "gateway-ts-sdk"
  }
}
