{
  "name": "openapi-ts-sdk-gateway",
  "version": "1.0.2",
  "description": "OpenAPI TypeScript SDK Gateway implementation - HTTP client based on Gateway WebSocket proxy",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "prebuild": "npm run clean",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "npm run build && npm run test:gateway",
    "test:gateway": "node tests/gateway-http-builder.test.js"
  },
  "keywords": [
    "openapi",
    "typescript",
    "sdk",
    "gateway",
    "websocket",
    "proxy",
    "http",
    "client"
  ],
  "author": "langgexyz",
  "license": "MIT",
  "dependencies": {
    "gateway-ts-sdk": "file:../gateway-ts-sdk",
    "openapi-ts-sdk": "https://github.com/langgexyz/openapi-ts-sdk.git#semver:^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
